     <body ng-app="HomepageModule" ng-controller="AlbumsController" ng-cloak>
     
     <div  ng-style="{'z-index' : User.Invisible ? 999:-1}" style="background-color:black;z-index:999;width:100%;height:100%;opacity:0.9;position:fixed;left:0;top:50;">
	</div>
     
   <%- include partials/navbar.ejs %>
   <h3>Albums</h3>
   
   
	 <div ng-init="getuser('<%-req.session.passport.user%>')"></div>
	 <div ng-init="getalbums('<%-req.session.passport.user%>')"></div>
	
	<div  style="display:flex" ng-repeat="album in myalbums track by $index">
	<span>
	<a href="/album/{{album.id}}"><h1> {{album.name}}</h1></a>
	
	</span>
	<span>
	Created at {{album.phrase}}<br>
		

	</span>
	
	</div>
	
	</body>
